FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;  

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 4.15e-05;
    }
    freestream
    {
        type            fixedValue;
        value           uniform 4.15e-05;
    }
    inviscidWall
    {
        type            symmetryPlane;
    }
    viscousWall
    {
        type            boundaryCorrectedFixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
